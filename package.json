{
  "name": "eliteshots",
  "version": "0.0.1",
  "private": true,
  "description": "A website for Elite Dangerous screenshots",
  "author": "Sayak Mukhopadhyay <mukhopadhyaysayak@gmail.com>",
  "scripts": {
    "lint": "vue-cli-service lint",
    "versionfile": "node versionFile.js",
    "builddev": "vue-cli-service build --watch",
    "buildprod": "vue-cli-service build",
    "dev": "vue-cli-service serve",
    "rundev": "cross-env NODE_ENV=development nodemon --inspect ./bin/start.js",
    "runprod": "cross-env PORT=4002 pm2 start process.json --env production",
    "start": "run-s versionfile buildprod runprod",
    "startdev": "run-s versionfile preparedev",
    "preparedev": "run-p builddev rundev",
    "startrun": "npm run dev"
  },
  "dependencies": {
    "@bugsnag/js": "^5.2.0",
    "@bugsnag/plugin-express": "^5.2.0",
    "@bugsnag/plugin-vue": "^6.0.0",
    "axios": "^0.18.0",
    "axios-retry": "^3.1.2",
    "backblaze-b2": "^1.3.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "fs-extra": "^7.0.1",
    "jimp": "^0.6.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.2.5",
    "morgan": "^1.8.2",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-frontier": "^0.1.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "vue": "^2.5.22",
    "vue-router": "^3.0.1",
    "vue2-dropzone": "^3.5.2",
    "vuetify": "^1.5.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "pm2": "^2.10.4",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5",
    "webpack-bugsnag-plugins": "SayakMukhopadhyay/webpack-bugsnag-plugins"
  }
}
